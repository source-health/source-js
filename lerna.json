{
  "$schema": "node_modules/lerna/schemas/lerna-schema.json",
  "useNx": true,
  "useWorkspaces": true,
  "version": "0.1.1",
  "packages": [
    "packages/*"
  ],
  "npmClient": "yarn",
  "ignoreChanges": [
    "CHANGELOG.md",
    "**/__tests__/**",
    "**/__mocks__/**",
    "**/__testfixtures__/**"
  ]
}