{
  "name": "@source-health/js-bridge",
  "version": "0.1.1",
  "license": "MIT",
  "description": "Zero-dependency typescript library for cross-frame communication",
  "private": false,
  "main": "./dist/index.js",
  "exports": {
    "import": "./dist/index.esnext.mjs",
    "require": "./dist/index.js"
  },
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build:bundle": "rollup -c rollup.config.js",
    "build:types": "tsc --emitDeclarationOnly",
    "build": "yarn build:types && yarn build:bundle",
    "check:lint": "eslint src/",
    "test": "jest"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@babel/preset-env": "^7.18.9",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-typescript": "^8.3.2",
    "@types/jest": "^28.1.6",
    "@types/jsdom": "^20.0.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "jsdom": "^20.0.0",
    "rollup": "^2.72.1",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^28.0.7",
    "tslib": "^2.4.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "gitHead": "abe472f6a1dcdbfa6e06b3d4b38b8c524854e9cc"
