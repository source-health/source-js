{
  "name": "@source-health/source-js",
  "version": "1.0.1-beta.0",
  "license": "MIT",
  "description": "TypeScript SDK for building modern, Source-powered patient experiences",
  "repository": {
    "type": "git",
    "url": "https://github.com/source-health/source-js"
  },
  "keywords": [
    "Source", "Source Health"
  ],
  "author": "Source Health, Inc",
  "bugs": {
    "url": "https://github.com/source-health/source-js/issues"
  },
  "homepage": "https://github.com/source-health/source-js#readme",

  "private": false,
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.esnext.mjs",
      "require": "./dist/index.js"
    },
    "./server": {
      "import": "./dist/server.mjs",
      "require": "./dist/server.js"
    }
  },
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build:bundle": "rollup -c rollup.config.js",
    "build:types": "tsc --emitDeclarationOnly",
    "build": "yarn build:types && yarn build:bundle",
    "check:lint": "eslint src/",
    "generate:queries": "graphql-codegen --config codegen.yml"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.js.map",
    "dist/**/*.mjs",
    "dist/**/*.mjs.map",
    "dist/**/*.d.ts"
  ],
  "dependencies": {
    "@source-health/js-bridge": "^1.0.0-alpha.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.18.9",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-typescript": "^8.3.2",
    "graphql": "^16.4.0",
    "rollup": "^2.72.1",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.4.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
