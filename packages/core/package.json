{
  "name": "@source-health/js",
  "version": "0.2.1",
  "license": "MIT",
  "description": "TypeScript SDK for building modern, Source-powered patient experiences",
  "private": false,
  "main": "./dist/index.js",
  "exports": {
    "import": "./dist/index.esnext.mjs",
    "require": "./dist/index.js"
  },
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build:bundle": "rollup -c rollup.config.js",
    "build:types": "tsc --emitDeclarationOnly",
    "build": "yarn build:types && yarn build:bundle",
    "check:lint": "eslint src/",
    "generate:queries": "graphql-codegen --config codegen.yml"
  },
  "files": [
    "build/**/*.js",
    "build/**/*.js.map",
    "build/**/*.d.ts"
  ],
  "dependencies": {
    "@source-health/js-bridge": "*"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.18.9",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-typescript": "^8.3.2",
    "graphql": "^16.4.0",
    "rollup": "^2.72.1",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.4.0"
  }
}
